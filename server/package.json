{
  "name": "prompt-generator-server",
  "version": "1.0.0",
  "description": "Express backend for MERN prompt generator",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "seed": "node scripts/seed.js",
    "db:migrate": "node scripts/migrate.js",
    "db:reset": "node scripts/resetDb.js"
  },
  "dependencies": {
    "express": "^4.18.0",
    "mongoose": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-node": "^4.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.0",
    "@types/jest": "^29.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "prompt",
    "optimization",
    "ai",
    "mern",
    "express",
    "mongodb"
  ]
}
